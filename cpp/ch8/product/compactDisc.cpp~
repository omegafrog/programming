#include "compactDisc.hpp"

compactDisc::compactDisc()  : Product()
{
  setTitle();
  setSinger();
}

void compactDisc::setTitle()
{
  cout << "title>>";
  cin >> title;
  cin.get();
}

void compactDisc::setSinger()
{
  cout << "singer>>";
  cin >> singer;
  cin.get();
}

string compactDisc::getTitle()
{
  return title;
}

string compactDisc::getSinger()
{
  return singer;
}

