#ifndef PRODUCT_H
#define PRODUCT_H

#include <iostream>
using namespace std;

class Product{
private:
  int id;
  string description;
  string producer;
  int cost;
public:
  Product();
  Product(int id, string description, string producer, int cost);

  int getId();
  string getDescription();
  string getProducer();
  int getCost();
  
};


#endif /* PRODUCT_H */
