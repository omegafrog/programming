#ifndef BOOK_H
#define BOOK_H

#include <iostream>
#include "product.hpp"
using namespace std;

class Book : public Product{
private:
  int isbn;
  string author;
  string title;
public:
  Book(int isbn, string author, string title);

  int getISBN();
  string getAuthor();
  string getTitle();
  
};



#endif /* BOOK_H */
