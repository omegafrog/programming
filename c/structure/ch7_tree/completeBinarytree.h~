#ifndef COMPLETE_BINARY_TREE_H
#define COMPLETE_BINARY_TREE_H

typedef int element;
typedef struct completeBinarynTree{
  element item;
  struct compBnTree *left, *right;
}compBnTree;

//traversal
//preorder 
void VLR(compBnTree *root);
//inorder
void LVR(compBnTree *root);
//postorder
void LRV(compBnTree *root);

//print tree
void printTree(compBnTree *root);
#endif

